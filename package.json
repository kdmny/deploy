{
  "name": "@invisible/deploy",
  "version": "1.0.13",
  "description": "Module to help deploy and run Google App Engine projects.",
  "main": "index.js",
  "bin": {
    "deploy": "./deploy.js",
    "sync": "./sync.js"
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint --fix .",
    "pretest": "yarn run lint",
    "test:jasmine": "cross-env NEW_RELIC_ENABLED=false NODE_ENV=test jasmine",
    "test": "nyc npm run test:jasmine",
    "posttest": "assert-version-bump"
  },
  "keywords": [],
  "author": "Leonardo Kewitz <leo@invisible.email>",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^1.2.1",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.4",
    "shelljs": "^0.7.8"
  },
  "devDependencies": {
    "@invisible/eslint-config": "^1.1.2",
    "@invisible/publish": "^1.1.6",
    "eslint": "^4.7.2",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jasmine-reporters": "^2.3.2",
    "nyc": "^13.1.0",
    "cross-env": "^5.2.0"
  },
  "nyc": {
    "include": [
      "./sync.js"
    ],
    "exclude": [],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ],
    "all": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "sourceMap": false,
    "instrument": true,
    "cache": true,
    "check-coverage": false
  }
}
